fun isXBigger(x, y) {
    if (x > y) {
        print "x (" + x + ") is bigger than y (" + y + ")";
    } else {
        print "y (" + y + ") is bigger than x (" + x + ")";
    }
}

isXBigger(1, 2);
isXBigger(2, 1);